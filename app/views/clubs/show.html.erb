<p style="color: green"><%= notice %></p>
<div class="text-center">
<%= render @club %> 

    <strong>Events:</strong>
    <%@club.events.each do |e| %> 
       <p> <%= e.title %> 
        <%= button_to "Show this event", e , method: :get, class: "btn btn-primary m-1"%>
       </p> 

    <% end %>
  

<div>
  <%= button_to "Edit this club", edit_club_path(@club), method: :get, class: "btn btn-warning m-1" %> 
  <%= button_to "Back to clubs", clubs_path, method: :get, class: "btn btn-secondary m-1"%>

  <%= button_to "Destroy this club", @club, method: :delete,  class: "btn btn-dark m-1" %>
</div>
</div>