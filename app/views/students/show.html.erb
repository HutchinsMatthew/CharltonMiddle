<p style="color: green"><%= notice %></p>

<%= render @student %>

<div>
  <%= button_to "Edit this student", edit_student_path(@student), method: :get, class: "btn btn-primary m-1" %> |
  <%= button_to "Back to students", students_path, method: :get, class: "btn btn-primary m-1" %>

  <%= button_to "Destroy this student", @student, method: :delete, class: "btn btn-primary m-1" %>
</div>
