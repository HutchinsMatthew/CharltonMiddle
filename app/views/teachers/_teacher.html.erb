<div id="<%= dom_id teacher %>" class="border border-black">
  <p class="border border-black">
    <strong>Name:</strong>
    <%= teacher.name %>
  </p> 
  <strong>Students:</strong>
  <% teacher.students.each do |s| %>
    <p>
      <%= link_to s.name, student_path(id:s.id) %>
    </p>
  <%end%>

</div>
